 {{#each galleries}}
<div class="galleryBlock">
  <div class="galleryText">
    <h2 class="eventName">{{eventName}}</h2>
    <p class="description">{{description}}</p>
  </div>
  <div class="photos">
    {{#each photos}}
    <img src={{photo}} alt="{{photo}}" class="galleryPhoto"> {{/each}}
  </div>
</div>
